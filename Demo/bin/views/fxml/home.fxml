            <Button fx:id="returnBikeBtn" disable="true" layoutX="1104.0" layoutY="38.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" text="Return Bike" />
            <TextField fx:id="barcode" layoutX="217.0" layoutY="38.0" prefHeight="40.0" prefWidth="466.0" promptText="Barcode" />
         </children></AnchorPane>
   </children>
</AnchorPane>
