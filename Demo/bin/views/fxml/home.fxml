<AnchorPane prefHeight="768.0" prefWidth="1366.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="150.0" prefWidth="1366.0">
         <children>
            <ImageView fx:id="ecoImage" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../assets/images/logo.png" />
               </image>
            </ImageView>
            <TextField fx:id="search" layoutX="201.0" layoutY="53.0" prefHeight="40.0" prefWidth="350.0" promptText="Name" />
            <Button fx:id="searchBtn" layoutX="943.0" layoutY="53.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="Search" />
            <TextField layoutX="573.0" layoutY="53.0" prefHeight="40.0" prefWidth="350.0" promptText="Address" />
         </children>
      </AnchorPane>
      <ScrollPane layoutY="150.0" prefHeight="518.0" prefWidth="1366.0">
         <content>
            <FlowPane fx:id="flowPaneDock" hgap="60.0" prefHeight="518.0" prefWidth="1366.0" vgap="50.0" />
         </content>
      </ScrollPane>
      
      
      <AnchorPane layoutY="668.0" prefHeight="100.0" prefWidth="1366.0">
         <children>
            <Button fx:id="viewBikeBtn" disable="true" layoutX="920.0" layoutY="38.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" text="View Your Bike" />
